{
  "name": "securedsoftcms",
  "version": "0.1.0",
  "description": "Full Stack CMS with Next,Payload,Tailwind",
  "main": "build/server.js",
  "author": "MD Mahbub Hasan",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "dev": "node server.js",
    "copy": "copyfiles -u 1 ./collections/**/* ./build/collections && copyfiles -u 1 ./globals/**/* ./build/globals && copyfiles *.js ./build",
    "build": "cross-env NODE_ENV_BUILD=true next build && payload build && npm run copy",
    "start1": "cross-env PAYLOAD_CONFIG_PATH=build/payload.config.js NODE_ENV=production node build/server.js",
    "buildstart": "npm run build && npm run start",
    "start": "cross-env NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.7",
    "@lottiefiles/react-lottie-player": "^3.5.2",
    "@mui/material": "^5.11.2",
    "@next/font": "^13.1.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "axios": "^1.2.5",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "daisyui": "^2.49.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "framer-motion": "^7.3.6",
    "lazy-load-images": "^1.0.1",
    "moment": "^2.29.4",
    "netlify-plugin-cypress": "^1.2.0",
    "next": "^13.1.1",
    "payload": "^1.4.1",
    "payload-s3-upload": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.7.1",
    "react-icons": "^4.6.0",
    "react-lazy-load-image-component": "^1.5.5",
    "react-responsive": "^9.0.2",
    "react-reveal": "^1.2.2",
    "react-slick": "^0.29.0",
    "react-slick-ios-fix": "^0.24.8",
    "slick-carousel": "^1.8.1",
    "tailwind-scrollbar-hide": "^1.1.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.11",
    "cypress": "^10.9.0",
    "eslint": "8.23.1",
    "eslint-config-next": "^13.1.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "4.9.4"
  }
}
